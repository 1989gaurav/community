<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <organization>
    <name>Neo4j</name>
    <url>http://neo4j.org/</url>
  </organization>

  <licenses>
    <license>
      <name>GNU Affero General Public License, Version 3</name>
      <url>http://www.gnu.org/licenses/agpl-3.0-standalone.html</url>
    </license>
  </licenses>

  <groupId>org.neo4j.release</groupId>
  <artifactId>release-project</artifactId>
  <version>1.0-SNAPSHOT</version>

  <name>Neo4j-release project</name>

  <description>Build process for packaged Neo4j products.</description>

  <packaging>pom</packaging>

  <modules>
    <module>build</module>
    <module>include</module>
    <module>integration</module>
    <module>assembly</module>
  </modules>

  <properties>
    <neo4j-release.version>1.2.M01</neo4j-release.version>
    <neo4j-kernel.version>1.2-${neo4j-release.version}</neo4j-kernel.version>
    <neo4j-index.version>1.2-${neo4j-release.version}</neo4j-index.version>
    <neo4j-shell.version>1.2-${neo4j-release.version}</neo4j-shell.version>
    <neo4j-remote-graphdb.version>0.8-${neo4j-release.version}</neo4j-remote-graphdb.version>
    <neo4j-online-backup.version>0.7-${neo4j-release.version}</neo4j-online-backup.version>
    <neo4j-udc.version>0.1-${neo4j-release.version}</neo4j-udc.version>
  </properties>

</project>
