<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="${project.artifactId}">
    <version position="left" />
    <bannerLeft>
        <name>${project.name}</name>
        <src>images/logos/logo-dark.png</src>
        <href>http://neo4j.org/</href>
    </bannerLeft>
    <body>
        <links>
            <item name="Neo4j home" href="http://neo4j.org/" />
            <item name="news" href="http://news.neo4j.org/" />
            <item name="wiki" href="http://wiki.neo4j.org/" />
            <item name="issues" href="https://trac.neo4j.org/" />
            <item name="components" href="http://components.neo4j.org/" />
            <item name="apidocs" href="http://api.neo4j.org/" />
        </links>
        <menu name="Neo4j ${project.short-name}">
            <item name="Introduction" href="index.html" />
            <item name="Basic usage" href="basic.html" />
            <item name="Basic index usage" href="basic-index.html" />
            <item name="Traversal" href="traversal.html" />
            <item name="Domain entities" href="entities.html" />
            <item name="Social network" href="social-network.html" />
            <item name="Graph algorithms" href="graphalgo.html" />
            <item name="JavaDocs" href="apidocs/index.html" />
            <item name="Wiki" href="http://wiki.neo4j.org/content/Getting_Started_With_Apoc" />
            <item name="Source Repository"
                href="https://svn.neo4j.org/examples/${short-name}/trunk/" />
        </menu>
        <menu ref="reports" />
